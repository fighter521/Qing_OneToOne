<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- 固定自身大小 -->
    <style name="w_wrap_h_wrap">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="text_style_big_40" parent="w_wrap_h_wrap">
        <item name="android:textSize">@dimen/text_size_40</item>
        <item name="android:textColor">#939393</item>
    </style>
    <style name="text_style_small_22" parent="w_wrap_h_wrap">
        <item name="android:shadowColor">@android:color/black</item>
        <item name="android:textSize">@dimen/margin_top_32</item>
        <item name="android:textColor">#939393</item>
    </style>

</resources>